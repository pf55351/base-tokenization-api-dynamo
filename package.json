{
  "name": "fhp-tokenization-api",
  "version": "1.0.0",
  "description": "AWS Lambda to tokenize file for FHP Holding",
  "scripts": {
    "lint": "eslint  --fix",
    "lint:updated": "pipe-git-updated --ext=js -- eslint --ignore-path .gitignore",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier --write",
    "dev": "clear && sls offline start --stage dev --reloadHandler",
    "deploy:prod": "sls deploy --stage prod",
    "deploy:test": "sls deploy --stage test"
  },
  "keywords": [],
  "author": "Francesco Primerano",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@serverless/eslint-config": "^5.1.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^22.10.7",
    "@types/qrcode": "^1.5.5",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "git-list-updated": "^1.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "serverless": "^4.5.3",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-s3-local": "^0.8.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "eslintConfig": {
    "extends": "@serverless/eslint-config/node",
    "root": true
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.731.1",
    "@aws-sdk/client-ssm": "^3.735.0",
    "@middy/core": "^6.0.0",
    "@middy/do-not-wait-for-empty-event-loop": "^6.0.0",
    "@middy/error-logger": "^6.0.0",
    "@middy/http-error-handler": "^6.0.0",
    "@middy/http-event-normalizer": "^6.0.0",
    "@middy/http-header-normalizer": "^6.0.0",
    "@middy/http-json-body-parser": "^6.0.0",
    "@middy/http-multipart-body-parser": "^6.0.0",
    "@middy/http-security-headers": "^6.0.0",
    "@middy/http-urlencode-path-parser": "^6.0.0",
    "@middy/input-output-logger": "^6.0.0",
    "@middy/validator": "^6.0.0",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "6.2.1",
    "add": "^2.0.6",
    "algosdk": "^3.1.0",
    "dynamoose": "^4.0.3",
    "http-errors": "^2.0.0",
    "pdf-lib": "^1.17.1",
    "pinata-web3": "^0.5.4",
    "qrcode": "^1.5.4"
  },
  "directories": {
    "doc": "docs"
  }
}
